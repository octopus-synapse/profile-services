{
  "name": "@octopus-synapse/profile-services",
  "version": "3.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fast": "oxlint src/ test/",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typecheck": "tsc --noEmit -p tsconfig.build.json",
    "test": "bun test",
    "test:unit": "bun test",
    "test:watch": "bun test --watch",
    "test:cov": "bun test --coverage",
    "test:debug": "bun test",
    "test:e2e": "jest --config test/jest-e2e.json --runInBand",
    "test:smoke": "bun test ./test/smoke",
    "test:integration": "bun test --config=bunfig.integration.toml",
    "test:integration:docker": "docker compose -f docker-compose.test.yml up -d && sleep 3 && bun run test:integration:run ; docker compose -f docker-compose.test.yml down",
    "test:integration:run": "DATABASE_URL=postgresql://postgres:postgres@localhost:5433/profile_test REDIS_HOST=localhost REDIS_PORT=6380 JWT_SECRET=test-secret NODE_ENV=test bun run db:setup:test && bun test --config=bunfig.integration.toml",
    "db:setup:test": "prisma generate && prisma db push --force-reset --accept-data-loss",
    "test:arch": "bun test ./test/architecture",
    "test:critical": "bun test --test-name-pattern critical",
    "test:fast": "bun test --bail",
    "test:changed": "bun test",
    "test:metrics": "bun test",
    "test:watch:verbose": "bun test --watch",
    "precommit": "bun run typecheck && bun run lint:fast && bun test && bun run test:smoke",
    "ci:lint": "bun run lint",
    "ci:test": "bun run test:integration",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:push": "prisma db push",
    "prisma:studio": "prisma studio",
    "prisma:seed": "bun run prisma/seed.ts",
    "db:setup": "prisma generate && prisma migrate deploy",
    "db:reset": "prisma migrate reset --force",
    "schemas:export": "bun run src/scripts/export-schemas.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "@apollo/server": "^5.2.0",
